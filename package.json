{
  "name": "pseudocode-support",
  "displayName": "Better PseudoCode Support",
  "description": "A simple VSCode plugin that adds support for the Pseudocode language used in cs ed in Germany",
  "version": "0.6.0",
  "publisher": "jotrorox",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jotrorox/Better-PseudoCode-Support"
  },
  "contributes": {
    "languages": [
      {
        "id": "pseudocode",
        "aliases": [
          "Pseudocode",
          "pc"
        ],
        "extensions": [
          ".pc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pseudocode",
        "scopeName": "source.pseudocode",
        "path": "./syntaxes/pseudocode.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "pseudocode",
        "path": "./snippets/pseudocode.code-snippets"
      }
    ]
  }
}
